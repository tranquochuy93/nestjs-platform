{
    "name": "mondayorsunday/nestjs-platform",
    "version": "0.0.12",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "check": "prettier --check \"src/**/*.ts\"",
        "start": "nest start",
        "wz-command": "wz-command",
        "start:dev": "cross-env TZ=UTC npm run prebuild && nest start --watch",
        "start:hot": "npm run prebuild && npm run bundle:migrations && npm run bundle:app",
        "start:debug": "npm run prebuild && nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config jest-e2e.json --testTimeout=60000 --forceExit --detectOpenHandles --passWithNoTests",
        "test:unit": "jest --config jest-unit.json --testTimeout=60000 --forceExit --detectOpenHandles",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/orm.config.ts",
        "migration": "npm run typeorm -- migration:run",
        "bundle:app": "cross-env TZ=UTC nest build --webpack --webpackPath webpack-hmr.config.js --watch",
        "bundle:migrations": "nest build --webpack --webpackPath webpack-migration.config.js && npm run migration",
        "lint-staged": "lint-staged",
        "prepare": "husky install"
    },
    "dependencies": {
        "@azure/ms-rest-js": "^2.7.0",
        "@azure/storage-blob": "12.17.0",
        "@grpc/grpc-js": "1.10.0",
        "@nestjs/axios": "3.0.1",
        "@nestjs/bull": "10.0.1",
        "@nestjs/cache-manager": "2.1.1",
        "@nestjs/common": "10.2.10",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "10.2.10",
        "@nestjs/jwt": "10.2.0",
        "@nestjs/microservices": "10.2.10",
        "@nestjs/passport": "10.0.3",
        "@nestjs/platform-express": "10.3.7",
        "@nestjs/platform-socket.io": "^10.3.0",
        "@nestjs/schedule": "4.0.0",
        "@nestjs/swagger": "7.1.17",
        "@nestjs/terminus": "10.2.0",
        "@nestjs/throttler": "5.1.1",
        "@nestjs/typeorm": "10.0.1",
        "@nestjs/websockets": "^10.3.0",
        "@opentelemetry/api": "1.7.0",
        "@opentelemetry/auto-instrumentations-node": "0.41.1",
        "@opentelemetry/instrumentation-http": "0.48.0",
        "@socket.io/redis-adapter": "^8.2.1",
        "@songkeys/nestjs-redis": "10.0.0",
        "@types/md5": "2.3.5",
        "@types/sanitize-html": "^2.9.5",
        "axios": "1.6.5",
        "bcrypt": "5.1.1",
        "bull": "4.12.0",
        "cache-manager": "5.3.2",
        "cache-manager-redis-yet": "4.1.2",
        "camelcase-keys": "9.1.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "cross-env": "7.0.3",
        "csv-stringify": "6.4.5",
        "dayjs": "1.11.10",
        "dotenv": "16.3.1",
        "exceljs": "4.4.0",
        "faker": "^5.5.3",
        "fast-csv": "4.3.6",
        "kafkajs": "2.2.4",
        "lodash": "4.17.21",
        "nestjs-i18n": "10.2.3",
        "passport": "0.7.0",
        "passport-headerapikey": "1.2.2",
        "passport-jwt": "4.0.1",
        "passport-local": "1.0.0",
        "pg": "8.11.3",
        "reflect-metadata": "0.1.13",
        "rimraf": "5.0.5",
        "rxjs": "7.8.1",
        "sanitize-html": "2.12.1",
        "sharp": "0.33.2",
        "snakecase-keys": "5.5.0",
        "typeorm": "0.3.20",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "18.4.4",
        "@commitlint/config-conventional": "18.4.4",
        "@nestjs/cli": "10.2.1",
        "@nestjs/schematics": "10.0.3",
        "@nestjs/testing": "10.3.0",
        "@types/bcrypt": "5.0.2",
        "@types/bull": "4.10.0",
        "@types/cron": "2.0.0",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.21",
        "@types/faker": "5.5.9",
        "@types/jest": "29.5.11",
        "@types/lodash": "4.14.202",
        "@types/multer": "1.4.11",
        "@types/node": "20.10.7",
        "@types/passport-jwt": "4.0.0",
        "@types/passport-local": "1.0.38",
        "@types/supertest": "2.0.16",
        "@typescript-eslint/eslint-plugin": "7.5.0",
        "@typescript-eslint/parser": "7.5.0",
        "auto-changelog": "2.4.0",
        "cspell": "8.3.2",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jest-formatting": "3.1.0",
        "eslint-plugin-prettier": "5.1.2",
        "eslint-plugin-sonarjs": "0.23.0",
        "glob": "8.1.0",
        "husky": "8.0.3",
        "javascript-obfuscator": "4.0.2",
        "jest": "29.7.0",
        "jest-extended": "4.0.2",
        "joi": "17.11.0",
        "lint-staged": "15.2.0",
        "prettier": "3.1.1",
        "supertest": "6.3.3",
        "ts-jest": "29.1.1",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.4.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
